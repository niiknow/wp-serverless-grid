{
  "name": "vue-wp-plugin-starter",
  "version": "0.1.0",
  "description": "A WordPress Vue.js starter plugin",
  "author": "Tom Noogen <tommywhen@yahoo.com>",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts && eslint src/**/*.vue",
    "lint-fix": "tslint --project ./tsconfig.json --fix src/**/*.ts && eslint --fix src/**/*.vue",
    "test": "jest"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@types/jest": "^27.4.0",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@vueform/multiselect": "^2.3.1",
    "autoprefixer": "^10.4.2",
    "axios": "^0.24.0",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "core-js": "^3.20.2",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "27.4.7",
    "jsdom": "^19.0.0",
    "laravel-mix": "^6.0.39",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "purecss-sass": "^2.0.6",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "ts-jest": "27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4",
    "vue": "^3.2.11",
    "vue-axios": "^3.4.0",
    "vue-dataset": "^3.4.2",
    "vue-loader": "^16.5.0",
    "vue-router": "^4.0.10",
    "webpack": "^5.65.0"
  }
}
